<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<Tab text="Create Department" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CreateDepartmentController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="412.0" prefWidth="663.0">
            <children>
                <TextField fx:id="departmentNumberCreate" layoutX="50.0" layoutY="40.0" promptText="DeptNo" />
                <TextField fx:id="departmentNameCreate" layoutX="50.0" layoutY="73.0" promptText="DeptName" />
                <TextField fx:id="departmentPostcodeCreate" layoutX="50.0" layoutY="107.0" promptText="DeptPostCode" />
                <TextField fx:id="departmentCityCreate" layoutX="50.0" layoutY="142.0" promptText="DeptCity" />
                <TextField fx:id="departmentManagerCreate" layoutX="50.0" layoutY="176.0" promptText="DeptManager" />
                <Button fx:id="departmentCreateButton" layoutX="48.0" layoutY="236.0" mnemonicParsing="false"
                        onAction="#createDept" text="Create"/>
                <Button fx:id="departmentEditButton" layoutX="47.0" layoutY="272.0" mnemonicParsing="false"
                        onAction="#editDept" text="Edit"/>
                <Button fx:id="departmentRemoveButton" layoutX="46.0" layoutY="307.0" mnemonicParsing="false"
                        onAction="#removeDept" text="Remove"/>
                <ListView fx:id="departmentList" layoutX="248.0" layoutY="32.0" prefHeight="300.0" prefWidth="270.0"/>
                <ListView fx:id="userList" layoutX="570.0" layoutY="32.0" prefHeight="300.0" prefWidth="270.0"/>
                <Label layoutX="250.0" layoutY="10.0" prefHeight="17.0" prefWidth="270.0" text="Department information">
                    <padding>
                        <Insets left="75.0"/>
                    </padding>
                <Label layoutX="570.0" layoutY="10.0" prefHeight="17.0" prefWidth="270.0" text="Department Employees">
                </Label>
                    <padding>
                        <Insets left="75.0"/>
                    </padding>
                </Label>
            </children>
        </AnchorPane>
    </content>
</Tab>